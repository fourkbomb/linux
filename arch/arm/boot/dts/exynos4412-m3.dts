#include "exynos4412-galaxy-s3.dtsi"

/ {
	compatible = "samsung,m3", "samsung,midas", "samsung,exynos4412", "samsung,exynos4";

	memory@40000000 {
		device_type = "memory";
		reg =  <0x40000000 0x80000000>;
	};
};

